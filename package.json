{
  "name": "nom-ts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "umd:main": "dist/nom-ts.umd.production.js",
  "module": "dist/nom-ts.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "typedoc": "typedoc src/index.ts --excludePrivate --mode file --theme markdown --out docs --platform docusaurus",
    "generate-readme": "type-literal -i src/index.ts -o generated.md"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@hasparus/tslint-config": "hasparus/tslint-config-hasparus",
    "@types/jest": "^24.0.15",
    "dts-jest": "^22.0.4",
    "husky": "^3.0.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-jest": "^24.0.2",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "transform": {
      ".*types.test.ts": "dts-jest/transform"
    },
    "globals": {
      "_dts_jest_": {
        "compiler_options": {
          "strict": true,
          "target": "es6"
        }
      }
    }
  }
}
